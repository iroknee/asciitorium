{
  "name": "asciitorium",
  "version": "0.1.16",
  "private": false,
  "description": "an ASCII ui framework for web + cli",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/asciitorium.es.js",
      "default": "./dist/asciitorium.es.js"
    },
    "./jsx": {
      "types": "./dist/jsx/index.d.ts",
      "import": "./dist/jsx/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx/jsx-runtime.d.ts",
      "import": "./dist/jsx/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx/jsx-dev-runtime.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "web": "vite",
    "cli": "tsx src/demo.tsx",
    "build": "vite build && tsc -p tsconfig.json",
    "preview": "vite preview",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "author": "Iroknee <tiroknee@gmail.com>",
  "keywords": [
    "ascii",
    "ui",
    "framework",
    "terminal",
    "typescript",
    "text-ui",
    "cli-ui",
    "retro"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.12",
    "figlet": "^1.8.1",
    "fs-extra": "^11.3.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.3"
  },
  "dependencies": {}
}

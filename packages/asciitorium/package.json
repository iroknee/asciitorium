{
  "name": "asciitorium",
  "version": "0.1.5",
  "description": "An ASCII-based UI framework for web and cli",
  "type": "module",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start:web": "vite",
    "start:cli": "tsx src/demo.tsx",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "format": "prettier --write .",
    "art": "node scripts/generate-art.js",
    "art:fonts": "node -e 'require(\"figlet\").fonts((_, f) => console.log(f.join(\"\\n\")))'"
  },
  "author": "Iroknee <tiroknee@gmail.com>",
  "keywords": [
    "ascii",
    "ui",
    "framework",
    "terminal",
    "typescript",
    "text-ui",
    "cli-ui",
    "retro"
  ],
  "license": "MIT",
  "devDependencies": {
    "figlet": "^1.8.1",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.12",
    "fs-extra": "^11.3.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.3"
  },
  "dependencies": {}
}
